        script(src='/js/jquery.dataTables.js', type='text/javascript')
        script(src='/js/dataTables.bootstrap4.js', type='text/javascript')
        script(src='/js/vendor.js', type='text/javascript')
        script(src='/js/scripts.js', type='text/javascript')
        block scripts